                "domain": "localhost",
                "applyGracePeriod": "true",
                "gracePeriod": "10" /*Time in seconds*/
            }
        },
        "UserWebApi": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "api/users",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "securityKey": "SecurityKeySecurityKey",
                "domain": "localhost",
                "applyGracePeriod": "true",
                "gracePeriod": "10" /*Time in seconds*/
            },
            "applicationUrl": "http://localhost:8611/"
        }
    }
